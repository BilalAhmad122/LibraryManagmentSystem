@model IEnumerable<Data_Base_Layer.BookTypeTable>

@{
    ViewBag.Title = "Books Catageries";
}

<p>
    @Html.ActionLink("Add Book catagry", "Create",null,new { @class = "btn btn-primary" })
</p>
<table class="table table-hover">
    <tr>
        <th>
          Book Type
        </th>
        <th>
           User
        </th>
        <th>Action</th>
    </tr>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BookTypeID })
        </td>
    </tr>
}

</table>
